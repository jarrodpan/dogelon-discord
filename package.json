{
	"name": "dogelon-discord",
	"version": "1.11.0-next",
	"description": "stock and helper bot for personal discord",
	"type": "commonjs",
	"bin": {
		"app": "./"
	},
	"engines": {
		"node": "17.3.0",
		"npm": "8.3.0"
	},
	"scripts": {
		"build": "tsc -b",
		"build:dev": "tsc -b -v -w",
		"postbuild": "npm test",
		"start": "node ./bin/app.js",
		"prestart:dev": "tsc -b",
		"start:dev": "nodemon --inspect ./bin/app.js",
		"changelog": "cross-env PRERELEASE=true jest --coverage --noStackTrace --detectOpenHandles version",
		"changelog:dev": "jest --coverage --noStackTrace --detectOpenHandles version",
		"test": "cross-env PRERELEASE=true jest --coverage --detectOpenHandles",
		"lint": "eslint . --ext .js,.jsx,.ts,.tsx",
		"prepare": "husky install"
	},
	"author": "Jarrod Pan",
	"license": "ISC",
	"dependencies": {
		"@discordjs/rest": "^0.3.0",
		"@types/node": "^17.0.23",
		"axios": "^0.26.0",
		"better-sqlite3": "^7.5.0",
		"discord-api-types": "^0.27.3",
		"discord.js": "^13.6.0",
		"dotenv": "^16.0.0",
		"pg": "^8.7.3"
	},
	"devDependencies": {
		"@babel/preset-typescript": "^7.16.7",
		"@types/eslint": "^8.4.1",
		"@types/jest": "^27.4.1",
		"@types/pg": "^8.6.5",
		"@typescript-eslint/eslint-plugin": "^5.13.0",
		"@typescript-eslint/parser": "^5.13.0",
		"cross-env": "^7.0.3",
		"eslint": "^8.10.0",
		"husky": "^7.0.4",
		"jest": "^27.5.1",
		"ts-jest": "^27.1.3",
		"ts-node": "^10.5.0",
		"typescript": "^4.6.2"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run test versionUpdated"
		}
	}
}
